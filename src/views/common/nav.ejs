<header class="main-header">
    <% var menus = [{name: 'Shop1', href:'/', id: 'shop'},
        {name: 'Add Product', href:'/admin/add-product', id:'add-product'},
        {name: '주문 내역', href:'/orders', id: 'orders'},
        {name: '장바구니', href:'/cart', id: 'cart'},
        {name: 'Login', id: 'login', href:'/login'}]%>
    <nav class="main-header__nav">
        <ul class="main-header__item-list">
            <% for (let menu of menus) { %>
            <li class="main-header__item"><a class="<%= pagePath === menu.href ? 'active': '' %>" href="<%= menu.href%>"id="<%= menu.id%>"><%= menu.name %></a></li>
            <% } %>
        </ul>
    </nav>
</header>
